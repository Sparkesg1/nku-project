<h1>Login</h1>
  <% if flash[:notice] %>
    <h1><div class="notice"><%= flash[:notice] %></div></h1>
  <% end %>
<%= form_tag sessions_path do %>
  <p>
    <%= label_tag :UserName %><br>
    <%= text_field_tag :email %>
  </p>

  <p>
    <%= label_tag :Password %><br>
    <%= text_field_tag :password %>
  </p>

  <p>
    <%= label_tag :Password %><br>
    <%= text_field_tag :password_confirmation %>
  </p>

  <p>
    <%= submit_tag "Log in" %>
  </p>
<% end %>